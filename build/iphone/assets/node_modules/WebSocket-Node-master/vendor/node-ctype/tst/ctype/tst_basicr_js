function test(){var e,t,r,o;for(t=new mod_ctype.Parser({endian:"little"}),o=new Buffer(4),o[0]=23,o[3]=42,r=t.readData([{x:{type:"uint8_t"}},{y:{type:"uint8_t",offset:3}}],o,0),ASSERT.equal(23,r.x),ASSERT.equal(42,r.y),o=new Buffer(23),e=0;23>e;e++)o[e]=0;o.write("Hello, world!"),r=t.readData([{x:{type:"char[20]"}}],o,0),o=new Buffer(4),o[0]=3,o[1]=36,o[2]=37,o[3]=38,r=t.readData([{y:{type:"uint8_t"}},{x:{type:"uint8_t[y]"}}],o,0),console.log(mod_sys.inspect(r,!0)),t.typedef("ssize_t","int32_t"),ASSERT.deepEqual({ssize_t:"int32_t"},t.lstypes()),r=t.readData([{x:{type:"ssize_t"}}],o,0),ASSERT.equal(639968259,r.x)}var mod_ctype=require("../../ctype"),ASSERT=require("assert"),mod_sys=require("sys");test();