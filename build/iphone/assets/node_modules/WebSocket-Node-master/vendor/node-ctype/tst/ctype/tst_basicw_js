function test(){var e,t,r,o;for(t=new mod_ctype.Parser({endian:"big"}),r=new Buffer(4),t.writeData([{x:{type:"uint8_t",value:23}},{y:{type:"uint8_t",offset:3,value:42}}],r,0),ASSERT.equal(23,r[0]),ASSERT.equal(42,r[3]),r=new Buffer(20),e=0;20>e;e++)r[e]=0;for(r.write("Hello, world!"),o=new Buffer(22),t.writeData([{x:{type:"char[20]",value:r}}],o,0),e=0;20>e;e++)ASSERT.equal(r[e],o[e]);r=new Buffer(4),t.writeData([{y:{type:"uint8_t",value:3}},{x:{type:"uint8_t[y]",value:[36,37,38]}}],r,0),console.log(mod_sys.inspect(r)),t.typedef("ssize_t","int32_t"),ASSERT.deepEqual({ssize_t:"int32_t"},t.lstypes())}var mod_ctype=require("../../ctype"),ASSERT=require("assert"),mod_sys=require("sys");