function ruint8(e,t,o){return e[o]}function ruint16(e,t,o){var i=0;return"big"==t?(i=e[o]<<8,i|=e[o+1]):(i=e[o],i|=e[o+1]<<8),i}function fixu32(e,t){return e*(1<<24)+t}function ruint32(e,t,o){var i=0;return"big"==t?(i=e[o+1]<<16,i|=e[o+2]<<8,i|=e[o+3],i=fixu32(e[o],i)):(i=e[o+2]<<16,i|=e[o+1]<<8,i|=e[o],i=fixu32(e[o+3],i)),i}function ruint64(e,t,o){var i=Array(2);return"big"==t?(i[0]=ruint32(e,t,o),i[1]=ruint32(e,t,o+4)):(i[0]=ruint32(e,t,o+4),i[1]=ruint32(e,t,o)),i}function rsint8(e,t,o){var i;return i=128&e[o],i?-1*(255-e[o]+1):e[o]}function rsint16(e,t,o){var i,r;return r=ruint16(e,t,o),i=32768&r,i?-1*(65535-r+1):r}function rsint32(e,t,o){var i,r;return r=ruint32(e,t,o),i=2147483648&r,i?-1*(4294967295-r+1):r}function rsint64(e,t,o){var i,r;return r=ruint64(e,t,o),(i=2147483648&r[0])?(r[0]=-1*(4294967295-r[0]),r[1]=-1*(4294967295-r[1]+1),r):r}function rfloat(e,t,o){var i,r,n,s,a=[],c=127,h=255;return"big"==t?(a[0]=e[o],a[1]=e[o+1],a[2]=e[o+2],a[3]=e[o+3]):(a[3]=e[o],a[2]=e[o+1],a[1]=e[o+2],a[0]=e[o+3]),i=128&a[0],r=(127&a[0])<<1,r|=(128&a[1])>>>7,n=(127&a[1])<<16,n|=a[2]<<8,n|=a[3],i||r!=h||0!==n?i&&r==h&&0===n?Number.NEGATIVE_INFINITY:r==h&&0!==n?Number.NaN:0===r&&0===n?0:(r-=c,r==-c?(r++,s=0):s=1,s=(s+n*Math.pow(2,-23))*Math.pow(2,r),i&&(s*=-1),s):Number.POSITIVE_INFINITY}function rdouble(e,t,o){var i,r,n,s,a,c=[],h=1023,u=2047;return"big"==t?(c[0]=e[o],c[1]=e[o+1],c[2]=e[o+2],c[3]=e[o+3],c[4]=e[o+4],c[5]=e[o+5],c[6]=e[o+6],c[7]=e[o+7]):(c[7]=e[o],c[6]=e[o+1],c[5]=e[o+2],c[4]=e[o+3],c[3]=e[o+4],c[2]=e[o+5],c[1]=e[o+6],c[0]=e[o+7]),i=128&c[0],r=(127&c[0])<<4,r|=(240&c[1])>>>4,a=c[7],a|=c[6]<<8,a|=c[5]<<16,n=c[4],n|=c[3]<<8,n|=c[2]<<16,n|=(15&c[1])<<24,n*=Math.pow(2,24),n+=a,i||r!=u||0!==n?i&&r==u&&0===n?Number.NEGATIVE_INFINITY:r==u&&0!==n?Number.NaN:0===r&&0===n?0:(r-=h,r==-h?(r++,s=0):s=1,s=(s+n*Math.pow(2,-52))*Math.pow(2,r),i&&(s*=-1),s):Number.POSITIVE_INFINITY}function prepuint(e,t){if("number"!=typeof e)throw new(Error("cannot write a non-number as a number"));if(0>e)throw Error("specified a negative value for writing an unsigned value");if(e>t)throw Error("value is larger than maximum value for type");if(Math.floor(e)!==e)throw Error("value has a fractional component");return e}function wuint8(e,t,o,i){var r;r=prepuint(e,255),o[i]=r}function wuint16(e,t,o,i){var r;r=prepuint(e,65535),"big"==t?(o[i]=(65280&r)>>>8,o[i+1]=255&r):(o[i+1]=(65280&r)>>>8,o[i]=255&r)}function wuint32(e,t,o,i){var r;r=prepuint(e,4294967295),"big"==t?(o[i]=(r-(16777215&r))/Math.pow(2,24),o[i+1]=255&r>>>16,o[i+2]=255&r>>>8,o[i+3]=255&r):(o[i+3]=(r-(16777215&r))/Math.pow(2,24),o[i+2]=255&r>>>16,o[i+1]=255&r>>>8,o[i]=255&r)}function wuint64(e,t,o,i){prepuint(e[0],4294967295),prepuint(e[1],4294967295),"big"==t?(wuint32(e[0],t,o,i),wuint32(e[1],t,o,i+4)):(wuint32(e[0],t,o,i+4),wuint32(e[1],t,o,i))}function prepsint(e,t,o){if("number"!=typeof e)throw new(Error("cannot write a non-number as a number"));if(e>t)throw Error("value larger than maximum allowed value");if(o>e)throw Error("value smaller than minimum allowed value");if(Math.floor(e)!==e)throw Error("value has a fractional component");return e}function wsint8(e,t,o,i){var r;r=prepsint(e,127,-240),r>=0?wuint8(r,t,o,i):wuint8(255+r+1,t,o,i)}function wsint16(e,t,o,i){var r;r=prepsint(e,32767,-61440),r>=0?wuint16(r,t,o,i):wuint16(65535+r+1,t,o,i)}function wsint32(e,t,o,i){var r;r=prepsint(e,2147483647,-4026531840),r>=0?wuint32(r,t,o,i):wuint32(4294967295+r+1,t,o,i)}function wsint64(e,t,o,i){var r=Array(2);prepsint(e[0],2147483647,-4026531840),prepsint(e[1],4294967295,-4294967295),0>e[0]||0>e[1]?(r[0]=4294967295-Math.abs(e[0]),r[1]=4294967296-Math.abs(e[1]),4294967296==r[1]&&(r[1]=0,r[0]++)):(r[0]=e[0],r[1]=e[1]),"big"==t?(wuint32(r[0],t,o,i),wuint32(r[1],t,o,i+4)):(wuint32(r[0],t,o,i+4),wuint32(r[1],t,o,i))}function log2(e){return Math.log(e)/Math.log(2)}function intexp(e){return Math.floor(log2(e))}function fracexp(e){return Math.floor(log2(e))}function wfloat(e,t,o,i){var r,n,s,a,c=[];isNaN(e)?(r=0,n=255,s=23):e==Number.POSITIVE_INFINITY?(r=0,n=255,s=0):e==Number.NEGATIVE_INFINITY?(r=1,n=255,s=0):(0>e?(r=1,e=Math.abs(e)):r=0,a=1>e?fracexp(e):intexp(e),-127>=a?(n=0,s=8388607&e*Math.pow(2,149)):(n=127+a,s=e*Math.pow(2,23-a),s&=8388607)),c[0]=r<<7|(254&n)>>>1,c[1]=(1&n)<<7|(8323072&s)>>>16,c[2]=(65280&s)>>>8,c[3]=255&s,"big"==t?(o[i]=c[0],o[i+1]=c[1],o[i+2]=c[2],o[i+3]=c[3]):(o[i]=c[3],o[i+1]=c[2],o[i+2]=c[1],o[i+3]=c[0])}function wdouble(e,t,o,i){var r,n,s,a,c=[];isNaN(e)?(r=0,n=2047,s=23):e==Number.POSITIVE_INFINITY?(r=0,n=2047,s=0):e==Number.NEGATIVE_INFINITY?(r=1,n=2047,s=0):(0>e?(r=1,e=Math.abs(e)):r=0,a=1>e?fracexp(e):intexp(e),2.225073858507201e-308>=e||-1023>=a?(n=0,s=e*Math.pow(2,1023)*Math.pow(2,51),s%=Math.pow(2,52)):(a>1023&&(a=1023),n=1023+a,s=e*Math.pow(2,-a),s*=Math.pow(2,52),s%=Math.pow(2,52))),c[7]=255&s,c[6]=255&s>>>8,c[5]=255&s>>>16,s=(s-(16777215&s))/Math.pow(2,24),c[4]=255&s,c[3]=255&s>>>8,c[2]=255&s>>>16,c[1]=(15&n)<<4|s>>>24,c[0]=r<<7|(2032&n)>>>4,"big"==t?(o[i]=c[0],o[i+1]=c[1],o[i+2]=c[2],o[i+3]=c[3],o[i+4]=c[4],o[i+5]=c[5],o[i+6]=c[6],o[i+7]=c[7]):(o[i+7]=c[0],o[i+6]=c[1],o[i+5]=c[2],o[i+4]=c[3],o[i+3]=c[4],o[i+2]=c[5],o[i+1]=c[6],o[i]=c[7])}exports.ruint8=ruint8,exports.ruint16=ruint16,exports.ruint32=ruint32,exports.ruint64=ruint64,exports.wuint8=wuint8,exports.wuint16=wuint16,exports.wuint32=wuint32,exports.wuint64=wuint64,exports.rsint8=rsint8,exports.rsint16=rsint16,exports.rsint32=rsint32,exports.rsint64=rsint64,exports.wsint8=wsint8,exports.wsint16=wsint16,exports.wsint32=wsint32,exports.wsint64=wsint64,exports.rfloat=rfloat,exports.rdouble=rdouble,exports.wfloat=wfloat,exports.wdouble=wdouble;