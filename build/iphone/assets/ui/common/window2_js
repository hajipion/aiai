function Window2(t){var e=Ti.UI.createWindow({title:t}),i=Ti.UI.createView({layout:"vertical"}),a=Ti.Map.createView({mapType:Ti.Map.STANDARD_TYPE,region:{latitude:35.645,longitutde:139.71,latitudeDelta:.01,longitudeDelta:.01},animate:!0,regionFit:!1,top:0,width:"auto",height:"100%"}),o=Ti.UI.createView({layout:"vertical",top:0,height:"30%",width:"100%",backgroundColor:"#000",opacity:.5}),r=Ti.UI.createTextField({color:"#333",hintText:"現在地",height:"auto",width:"60%",top:10,borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED}),d=Ti.UI.createTextField({color:"#333",hintText:"目的地",height:"auto",width:"60%",top:10,borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED}),n=Ti.UI.createButton({title:"HELP!",width:"100%",height:"auto"}),l=Ti.UI.createAlertDialog({title:"HELP?",message:"近くの人に助けを求めますか？",buttonNames:["OK","Cancel"],cancel:1});return n.addEventListener("click",function(){l.show()}),i.add(a),a.add(o),o.add(r),o.add(d),o.add(n),e.add(i),e}module.exports=Window2;