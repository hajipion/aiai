function Window2(e){var t=Ti.UI.createWindow({title:e}),i=Ti.UI.createView({});if(!Ti.Geolocation.locationServicesEnabled)return Ti.UI.createAlertDialog({title:"位置情報取得",message:"位置測定が出来ません。電波状況、設定を確認してください。"}),void 0;var a=Ti.UI.createView({layout:"vertical",top:0,height:"40%",width:"auto",backgroundColor:"#000",opacity:.7}),o=Ti.UI.createTextField({color:"#333",hintText:"現在地",width:"60%",top:10,editable:!1,borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED}),r=Ti.UI.createTextField({color:"#333",hintText:"目的地を入力",width:"60%",top:10,borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED}),d=Ti.UI.createButton({top:10,width:55,height:55,backgroundImage:"/images/help.png",opacity:1}),n=Ti.UI.createAlertDialog({buttonNames:["OK","Cancel"],cancel:1});d.addEventListener("click",function(){""===r.getValue()?(n.setTitle("HELPする前に！"),n.setMessage("目的地を入力してください。"),n.show()):(n.setTitle("HELP?"),n.setMessage("近くの人に助けを求めますか？"),n.show())});var l=Ti.Map.createView({mapType:Ti.Map.STANDARD_TYPE,region:{latitude:35.681382,longitude:139.76608,latitudeDelta:.01,longitudeDelta:.01},animate:!0,regionFit:!0,width:"auto",height:"auto"});if(i.add(l),t.add(i),"android"==Ti.Platform.name){var c=Ti.Geolocation.Android.createLocationProvider({name:Ti.Geolocation.PROVIDER_GPS,minUpdateDistance:0,minUpdateTime:0});Ti.Geolocation.Android.addLocationProvider(c),Ti.Geolocation.Android.manualMode=!0}var s=function(e){if(e.success&&!e.error){var t=e.coords.latitude,i=e.coords.longitude,a=Math.round(100*t)/100,r=Math.round(100*i)/100;o.setValue("現在地："+a+","+r);var d=Ti.Map.createAnnotation({latitude:t,longitude:i,pincolor:Ti.Map.ANNOTATION_RED,pinImage:"/images/map-pin.png",animate:!0});l.addAnnotation(d),l.show(),l.setLocation({latitude:t,longitude:i,animate:!0,latitudeDelta:.01,longitudeDelta:.01})}};return Ti.Geolocation.addEventListener("location",s),i.add(a),a.add(o),a.add(r),a.add(d),t.add(i),t}module.exports=Window2;