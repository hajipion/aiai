function Window3(e){for(var t=Ti.UI.createWindow({title:e}),i=Ti.UI.createView({backgroundImage:"/images/bg_sample.png"}),a=Ti.UI.createView({height:"35%",width:"auto",fill:!1,top:0}),o=Ti.UI.createImageView({image:"/images/icon_umbrella.png",width:130,top:10}),r=Ti.UI.createView({height:"65%",width:"auto",fill:!1,bottom:0}),d=Ti.UI.createView({backgroundColor:"#dcdcdc",opacity:.5}),n=Ti.UI.createView({height:"90%",width:"80%",fill:!1}),l=Ti.App._withFriends,c=[],u=0;l.length>u;u++){var s=l[u],h=Ti.UI.createTableViewRow({height:60}),T=Ti.UI.createLabel({font:{fontSize:20},color:"#000",top:5}),p=Ti.UI.createView({width:"100%",layout:"horizontal",top:30,left:60}),g=Ti.UI.createLabel({width:"40%",font:{fontSize:13},color:"#666",fontWeight:"bold"}),w=Ti.UI.createLabel({width:"30%",font:{fontSize:17},color:"#38695A",fontWeight:"bold",bottom:4});T.text=s.text,g.text="♥紳士ポイント ",w.text=s.pt+"pt",p.add(g),p.add(w),h.add(T),h.add(p),c.push(h)}var m=Ti.UI.createTableView({opacity:.5,data:c});return m.addEventListener("click",function(e){var i=require("ui/common/profile"),a=new i(t,l[e.index]);t.containingTab.open(a,{animated:!0})}),n.add(d),n.add(m),a.add(o),r.add(n),i.add(a),i.add(r),t.add(i),t}module.exports=Window3;