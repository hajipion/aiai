function Window3(e){for(var t=Ti.UI.createWindow({title:e}),n=Ti.UI.createView({backgroundImage:"/images/bg_sample.png"}),o=Ti.UI.createView({height:"35%",width:"auto",fill:!1,top:0}),r=Ti.UI.createImageView({image:"/images/icon_umbrella.png",width:130,top:10}),i=Ti.UI.createView({height:"65%",width:"auto",fill:!1,bottom:0}),s=Ti.UI.createView({backgroundColor:"#dcdcdc",opacity:.5}),a=Ti.UI.createView({height:"90%",width:"80%",fill:!1}),c=Ti.App._withFriends,u=[],l=0;c.length>l;l++){var d=c[l],p=Ti.UI.createTableViewRow({height:60}),f=Ti.UI.createLabel({font:{fontSize:20},color:"#000",top:5}),h=Ti.UI.createView({width:"100%",layout:"horizontal",top:30,left:60}),m=Ti.UI.createLabel({width:"40%",font:{fontSize:13},color:"#666",fontWeight:"bold"}),g=Ti.UI.createLabel({width:"30%",font:{fontSize:17},color:"#38695A",fontWeight:"bold",bottom:4});f.text=d.text,m.text="♥紳士ポイント ",g.text=d.pt+"pt",h.add(m),h.add(g),p.add(f),p.add(h),u.push(p)}var y=Ti.UI.createTableView({opacity:.5,data:u});return y.addEventListener("click",function(e){var n=require("ui/common/profile"),o=new n(t,c[e.index]);t.containingTab.open(o,{animated:!0})}),a.add(s),a.add(y),o.add(r),i.add(a),n.add(o),n.add(i),t.add(n),t}module.exports=Window3;