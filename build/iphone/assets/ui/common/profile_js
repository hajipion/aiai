function ProfileWindow(e,i){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"プロフィール"}),a=Ti.UI.createView({backgroundImage:"/images/bg_sample.png"}),r=Ti.UI.createView({height:"35%",width:"auto",fill:!1,top:0}),o=Ti.UI.createImageView({image:"/images/icon_umbrella.png",width:130,top:10}),n=Ti.UI.createView({height:"83%",width:"auto",fill:!1,top:"22%",bottom:0}),d=Ti.UI.createView({backgroundColor:"#dcdcdc",opacity:.7,top:60,width:"80%",height:"70%"}),s=Ti.UI.createView({width:"100%",fill:!1,top:0}),l=[{text:"相合仲間 ",num:"24人"},{text:"助けた回数 ",num:"2回"},{text:"助けてもらった回数 ",num:"2回"},{text:"紳士ポイント ",num:i.pt+"pt"}],c=Ti.UI.createLabel({text:i.text,top:20,font:{fontSize:23},color:"#000",fontWeight:"bold"});s.add(c);for(var u=Ti.UI.createView({height:"75%",top:65,bottom:0,layout:"vertical"}),p=0;l.length>p;p++){var g=Ti.UI.createLabel({left:20,width:"65%",color:"#666"}),h=Ti.UI.createLabel({left:15,width:"20%",color:"#38695A",font:{fontSize:20}}),T=Ti.UI.createView({width:"100%",height:"21%",bottom:0,layout:"horizontal"});g.text=l[p].text,h.text=l[p].num,T.add(g),T.add(h),u.add(T)}return s.add(u),r.add(o),d.add(s),n.add(d),a.add(r),a.add(n),t.add(a),t}module.exports=ProfileWindow;