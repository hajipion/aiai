function ProfileWindow(e,t){var n=Ti.UI.createWindow({backgroundColor:"#fff",title:"プロフィール"}),o=Ti.UI.createView({backgroundImage:"/images/bg_sample.png"}),r=Ti.UI.createView({height:"35%",width:"auto",fill:!1,top:0}),i=Ti.UI.createImageView({image:"/images/icon_umbrella.png",width:130,top:10}),s=Ti.UI.createView({height:"83%",width:"auto",fill:!1,top:"22%",bottom:0}),a=Ti.UI.createView({backgroundColor:"#dcdcdc",opacity:.7,top:60,width:"80%",height:"70%"}),c=Ti.UI.createView({width:"100%",fill:!1,top:0}),u=[{text:"相合仲間 ",num:"24人"},{text:"助けた回数 ",num:"2回"},{text:"助けてもらった回数 ",num:"2回"},{text:"紳士ポイント ",num:t.pt+"pt"}],l=Ti.UI.createLabel({text:t.text,top:20,font:{fontSize:23},color:"#000",fontWeight:"bold"});c.add(l);for(var d=Ti.UI.createView({height:"75%",top:65,bottom:0,layout:"vertical"}),p=0;u.length>p;p++){var f=Ti.UI.createLabel({left:20,width:"65%",color:"#666"}),h=Ti.UI.createLabel({left:15,width:"20%",color:"#38695A",font:{fontSize:20}}),m=Ti.UI.createView({width:"100%",height:"21%",bottom:0,layout:"horizontal"});f.text=u[p].text,h.text=u[p].num,m.add(f),m.add(h),d.add(m)}return c.add(d),r.add(i),a.add(c),s.add(a),o.add(r),o.add(s),n.add(o),n}module.exports=ProfileWindow;