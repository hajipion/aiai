function FirstView(){var i=Ti.UI.createView(),e=Ti.UI.createTextField({top:10,height:"auto",width:"90%",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,hintText:"Name"});i.add(e);var a=Ti.UI.createTextField({top:50,height:"auto",width:"90%",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,passwordMask:!0,hintText:"Password"});i.add(a);var t=Ti.UI.createButton({top:100,height:"auto",width:200,title:"ユーザー作成"}),o=require("ti.cloud");o.debug=!0;var r,n;return t.title="ユーザー作成",t.addEventListener("click",function(){r=e.value,n=a.value,o.Users.create({username:r,password:n,password_confirmation:n},function(i){if(i.success){var e=Ti.UI.createWindow({backgroundColor:"#ffffff"}),a=Ti.UI.createLabel({top:10,width:"auto",height:"auto",text:"ユーザーを作成しました。"});e.add(a),e.open()}else alert("Faild to create user! "+i.message)})}),i.add(t),i}module.exports=FirstView;