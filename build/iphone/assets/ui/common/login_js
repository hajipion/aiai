function FirstView(e){function t(t,n){var o=require("ti.cloud");o.debug=!0,o.Users.create({username:t,password:n,password_confirmation:n},function(o){if(o.success){Ti.App.Properties.setString("username",t),Ti.App.Properties.setString("pass",n),alert("user つくった");var r=require("ui/common/Push_and");r.Push_and();var i=require("ui/common/ApplicationTabGroup");new i(e).open()}else new i(e).open(),alert("Faild to create user! "+o.message)})}var n=Ti.UI.createWindow({backgroundImage:"/images/login-back.png"}),o=Ti.UI.createView({top:120,layout:"vertical",height:"27%",width:"70%"}),r=Ti.UI.createView({layout:"absolute",width:"100%",height:"25%"});if("android"==Ti.Platform.name)var i=Ti.UI.createImageView({image:"/images/login-name.png",height:"100%",left:0});else var i=Ti.UI.createImageView({image:"/images/login-name.png",height:"100%",left:-25});var s=Ti.UI.createTextField({right:0,height:"100%",width:"80%",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,hintText:"Name"});r.add(i),r.add(s);var a=Ti.UI.createView({layout:"absolute",width:"100%",height:"25%",top:10});if("android"==Ti.Platform.name)var c=Ti.UI.createImageView({image:"/images/login-name.png",height:"100%",left:0});else var c=Ti.UI.createImageView({image:"/images/login-pass.png",height:"100%",left:-25});var u=Ti.UI.createTextField({right:0,height:"100%",width:"80%",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,passwordMask:!0,hintText:"Password"});a.add(c),a.add(u),o.add(r),o.add(a);var l=Ti.UI.createButton({color:"#fff",top:20,height:"25%",width:"100%",title:"ユーザー作成",backgroundImage:"/images/back-login-button.png"});return o.add(l),l.title="新規登録",l.addEventListener("click",function(){var e,n;e=s.value,n=u.value,e&&n?t(e,n):alert("入力してよ！")}),n.add(o),n}module.exports=FirstView;