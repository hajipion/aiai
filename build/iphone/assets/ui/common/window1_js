function Window1(e){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:e}),i=Ti.UI.createView({backgroundImage:"/images/bg_sample.png"}),a=Ti.UI.createView({height:"35%",width:"auto",fill:!1,top:0}),o=Ti.UI.createView({opacity:0,height:"90%",width:"80%",fill:!1}),r=Ti.UI.createImageView({image:"/images/icon_umbrella.png",width:130,top:10,bottom:10}),d=Ti.UI.createView({height:"80%",width:"auto",fill:!1,bottom:0}),n=Ti.UI.createView({backgroundColor:"#dcdcdc",opacity:.7,top:60,width:"80%",height:"70%"}),l=Ti.UI.createView({width:"100%",fill:!1,top:0}),c=[{text:"相合仲間 ",num:"24人"},{text:"助けた回数 ",num:"2回"},{text:"助けてもらった回数 ",num:"2回"},{text:"紳士度 ",num:"10%"}],u=Ti.UI.createLabel({text:"山田 太郎",top:20,font:{fontSize:23},color:"#000",fontWeight:"bold"});l.add(u);for(var w=Ti.UI.createView({height:"75%",top:65,bottom:0,layout:"vertical"}),h=0;c.length>h;h++){var T=Ti.UI.createLabel({left:20,width:"65%",color:"#666"}),p=Ti.UI.createLabel({left:15,width:"20%",color:"#38695A",font:{fontSize:20}}),g=Ti.UI.createView({width:"100%",height:"21%",bottom:0,layout:"horizontal"});T.text=c[h].text,p.text=c[h].num,g.add(T),g.add(p),w.add(g)}return l.add(w),a.add(o),a.add(r),n.add(l),d.add(n),i.add(a),i.add(d),t.add(i),t}module.exports=Window1;