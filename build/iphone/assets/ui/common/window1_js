function Window1(e){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:e}),i=Ti.UI.createView({backgroundImage:"/images/bg_sample.png"}),a=Ti.UI.createView({height:"35%",width:"auto",fill:!1,top:0}),o=Ti.UI.createImageView({image:"/images/icon_umbrella.png",width:130,top:10}),r=Ti.UI.createView({height:"83%",width:"auto",fill:!1,top:"22%",bottom:0}),d=Ti.UI.createView({backgroundColor:"#dcdcdc",opacity:.7,top:60,width:"80%",height:"70%"}),n=Ti.UI.createView({width:"100%",fill:!1,top:0}),l=[{text:"相合仲間 ",num:"24人"},{text:"助けた回数 ",num:"2回"},{text:"助けてもらった回数 ",num:"2回"},{text:"紳士ポイント ",num:"10pt"}],c=Ti.UI.createLabel({text:"山田 太郎",top:20,font:{fontSize:23},color:"#000",fontWeight:"bold"});n.add(c);for(var u=Ti.UI.createView({height:"75%",top:65,bottom:0,layout:"vertical"}),w=0;l.length>w;w++){var h=Ti.UI.createLabel({left:20,width:"65%",color:"#666"}),T=Ti.UI.createLabel({left:15,width:"20%",color:"#38695A",font:{fontSize:20}}),p=Ti.UI.createView({width:"100%",height:"21%",bottom:0,layout:"horizontal"});h.text=l[w].text,T.text=l[w].num,p.add(h),p.add(T),u.add(p)}return n.add(u),a.add(o),d.add(n),r.add(d),i.add(a),i.add(r),t.add(i),t}module.exports=Window1;