function TileView(e){for(var i,t=Ti.UI.createWindow({title:e}),a=Ti.UI.createView({backgroundColor:"#fff",height:"auto",width:"auto",fill:!1,top:0}),r=[],o=3,n=Ti.Platform.displayCaps.platformWidth/o,d=n,l=0;18>l;l++){if(0===l||0===l%o){l>0&&r.push(i),i=Ti.UI.createTableViewRow({height:d});var s=Ti.UI.createView({layout:"horizontal",width:Ti.UI.FILL,height:d});i.add(s)}var c=Ti.UI.createView({width:n,height:d}),u=Ti.UI.createView({borderColor:"black",borderWidth:3,borderRadius:5,height:"90%",width:"90%"});c.add(u),s.add(c)}r.push(i);var p=Ti.UI.createTableView({separatorStyle:0,separatorColor:"#fff",data:r,width:Ti.UI.FILL});return a.add(p),t.add(a),t}module.exports=TileView;