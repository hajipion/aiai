function TileView(e){for(var t,n=Ti.UI.createWindow({title:e}),o=Ti.UI.createView({backgroundColor:"#fff",height:"auto",width:"auto",fill:!1,top:0}),r=[],i=3,s=Ti.Platform.displayCaps.platformWidth/i,a=s,c=0;18>c;c++){if(0===c||0===c%i){c>0&&r.push(t),t=Ti.UI.createTableViewRow({height:a});var u=Ti.UI.createView({layout:"horizontal",width:Ti.UI.FILL,height:a});t.add(u)}var l=Ti.UI.createView({width:s,height:a}),d=Ti.UI.createView({borderColor:"black",borderWidth:3,borderRadius:5,height:"90%",width:"90%"});l.add(d),u.add(l)}r.push(t);var p=Ti.UI.createTableView({separatorStyle:0,separatorColor:"#fff",data:r,width:Ti.UI.FILL});return o.add(p),n.add(o),n}module.exports=TileView;