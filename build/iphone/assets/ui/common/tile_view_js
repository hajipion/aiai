function TileView(e){for(var t,i=Ti.UI.createWindow({title:e}),a=Ti.UI.createView({backgroundColor:"#fff",height:"auto",width:"auto",fill:!1,top:0}),o=[],r=3,d=Ti.Platform.displayCaps.platformWidth/r,n=d,l=0;18>l;l++){if(0===l||0===l%r){l>0&&o.push(t),t=Ti.UI.createTableViewRow({height:n});var c=Ti.UI.createView({layout:"horizontal",width:Ti.UI.FILL,height:n});t.add(c)}var u=Ti.UI.createView({width:d,height:n}),T=Ti.UI.createView({borderColor:"black",borderWidth:3,borderRadius:5,height:"90%",width:"90%"});u.add(T),c.add(u)}o.push(t);var w=Ti.UI.createTableView({separatorStyle:0,separatorColor:"#fff",data:o,width:Ti.UI.FILL});return a.add(w),i.add(a),i}module.exports=TileView;