function buyView(e){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:e}),n=Ti.UI.createView({layout:"vertical",backgroundImage:"/images/bg_sample.png"});t.add(n);var o="http://shopping.yahooapis.jp/ShoppingWebService/V1/json/itemSearch",r=Ti.Network.createHTTPClient({onload:function(){Ti.API.info("Received text: "+this.responseText),alert("success")},onerror:function(e){Ti.API.debug(e.error),alert("error")},timeout:5e3});return r.open("GET",o),r.send(),t}module.exports=buyView;