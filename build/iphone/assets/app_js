1.8>Ti.version&&alert("Sorry - this application template requires Titanium Mobile SDK 1.8 or later"),function(){function e(){var e=require("ti.cloud");e.debug=!0,Ti.App._withFriends=[];var r=require("ui/common/ApplicationTabGroup"),n=Ti.App.Properties.getString("username");Ti.App._username=n;var s=Ti.App.Properties.getString("pass");Ti.App.Properties.setString("username",o),e.Users.login({login:n,password:s},function(n){if(n.success){var s=n.users[0];Ti.App._userid=s.id,alert("Success:\\nid: "+s.id+"\\n"+"first name: "+s.first_name+"\\n"+"last name: "+s.last_name),e.debug=!0,e.Friends.search({user_id:Ti.App._userid},function(e){if(e.success&&e.users.length>0)for(var n=0;e.users.length>n;n++){var s=e.users[n],a=Ti.App._withFriends,o={text:s.username,pt:"50pt"};a.push(o),Ti.App._withFriends=a,new r(i).open()}else new r(i).open()})}else alert("ログインできないでーすユーザくつくりまーす！:\\n"+(n.error&&n.message||JSON.stringify(n)))})}var i,r=Ti.Platform.osname,n=(Ti.Platform.version,Ti.Platform.displayCaps.platformHeight),s=Ti.Platform.displayCaps.platformWidth,a="ipad"===r||"android"===r&&(s>899||n>899);i=a?require("ui/tablet/ApplicationWindow"):require("ui/handheld/ApplicationWindow");var o=Ti.App.Properties.getString("username");if(Ti.App.Properties.getString("pass"),"iPhone OS"==Ti.Platform.name)if(o){var p=require("ui/common/ApplicationTabGroup");e()}else{var p=require("ui/common/ios_login");new p(i).open()}else if(o){var t=require("ui/common/Push_and");t.Push_and();var p=require("ui/common/ApplicationTabGroup");new p(i).open()}else{var p=require("ui/common/login");new p(i).open()}}();